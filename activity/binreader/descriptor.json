{
    "name": "flogo-binreader",
    "type": "flogo:activity",
    "ref": "github.com/maginga/example/activity/binreader",
    "version": "0.0.2",
    "title": "Binary Reader Activity",
    "description": "Reading from binary file",
    "settings":[
      {
        "name": "columns",
